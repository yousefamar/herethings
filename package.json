{
  "name": "herethings",
  "version": "0.1.0",
  "description": "A set of online tools for quickly uploading sketches, webcam snaps, voice recordings, and more",
  "scripts": {
    "compile": "lsc -co build src/server.ls & webpack --progress --colors & wait",
    "watch": "lsc -cwo build src/server.ls & webpack --watch --progress --colors & wait",
    "start": "node build/server.js",
    "clean": "rm -rf build/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Paraknight/herethings.git"
  },
  "author": "Yousef Amar <yousef@amar.io> (http://yousefamar.com/)",
  "contributors": [
    "Yousef Amar <yousef@amar.io> (http://yousefamar.com/)",
    "Philipp Lenssen <philipp.lenssen@gmail.com>",
    "Scott Lowe <bgsosh@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/Paraknight/herethings/issues"
  },
  "homepage": "https://github.com/Paraknight/herethings#readme",
  "devDependencies": {
    "css-loader": "^0.19.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.0.3",
    "imagemin": "^3.2.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.7.1",
    "livescript": "^1.4.0",
    "livescript-loader": "^0.1.5",
    "style-loader": "^0.12.4",
    "stylus-loader": "^1.3.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2"
  },
  "dependencies": {
    "express": "^4.13.3"
  }
}
