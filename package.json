{
  "name": "herethings",
  "version": "0.4.0",
  "description": "A set of online tools for quickly uploading sketches, webcam snaps, voice recordings, and more",
  "scripts": {
    "compile": "lsc -co build/server src/server && webpack --progress --colors",
    "watch": "lsc -cwo build/server src/server & webpack --watch --progress --colors & wait",
    "watch-win": "start lsc -cwo build/server src/server & start webpack --watch --progress --colors",
    "start": "node build/server/server.js",
    "clean": "rm -rf build/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Paraknight/herethings.git"
  },
  "author": "Yousef Amar <yousef@amar.io> (http://yousefamar.com/)",
  "contributors": [
    "Yousef Amar <yousef@amar.io> (http://yousefamar.com/)",
    "Philipp Lenssen <philipp.lenssen@gmail.com>",
    "Scott Lowe <bgsosh@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/Paraknight/herethings/issues"
  },
  "homepage": "https://github.com/Paraknight/herethings#readme",
  "devDependencies": {
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.21.0",
    "ignore-loader": "^0.1.1",
    "imagemin": "^5.2.1",
    "jquery-ui": "^1.10.5",
    "json-loader": "^0.5.4",
    "livescript": "^1.5.0",
    "livescript-loader": "*",
    "pug-loader": "^1.0.2",
    "recordrtc": "^5.3.8",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.13.0",
    "multiparty": "^4.1.2",
    "pug": "^2.0.0-beta3"
  }
}
